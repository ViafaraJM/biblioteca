<settings>
    <servers>
        <server>
            <id>central-proxy</id>
            <configuration>
                <httpHeaders>
                    <property>
                        <name>Private-Token</name>
                        <value>${env.GITLAB_PERSONAL_TOKEN}</value>
                    </property>
                </httpHeaders>
            </configuration>
        </server>
    </servers>
    <mirrors>
        <mirror>
            <id>central-proxy</id>
            <name>GitLab proxy of central repo</name>
            <url>https://gitlab.com/api/v4/projects/biblioteca/packages/maven</url>
            <mirrorOf>central</mirrorOf>
        </mirror>
    </mirrors>
</settings>